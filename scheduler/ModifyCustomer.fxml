<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduler.ModifyCustomerController">
   <children>
      <GridPane layoutX="100.0" layoutY="32.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="108.0" minHeight="10.0" prefHeight="106.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="201.0" minHeight="10.0" prefHeight="201.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="139.0" minHeight="10.0" prefHeight="93.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Modify Customer">
               <font>
                  <Font size="20.0" />
               </font>
               <GridPane.margin>
                  <Insets left="15.0" />
               </GridPane.margin>
            </Label>
            <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <TextField fx:id="txtName" prefHeight="25.0" prefWidth="210.0" promptText="Name">
                     <FlowPane.margin>
                        <Insets left="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <TextField fx:id="txtPhone" promptText="Phone">
                     <FlowPane.margin>
                        <Insets left="10.0" top="10.0" />
                     </FlowPane.margin>
                  </TextField>
               </children>
            </FlowPane>
            <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <TextField fx:id="txtAddress" prefHeight="25.0" prefWidth="277.0" promptText="Address" />
                  <TextField fx:id="txtAddress2" prefHeight="25.0" prefWidth="277.0" promptText="Address 2">
                     <FlowPane.margin>
                        <Insets top="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <ComboBox fx:id="comboCity" onAction="#comboCity_onAction" prefHeight="25.0" prefWidth="170.0" promptText="City">
                     <FlowPane.margin>
                        <Insets right="10.0" top="10.0" />
                     </FlowPane.margin>
                  </ComboBox>
                  <TextField fx:id="txtPostal" prefHeight="25.0" prefWidth="118.0" promptText="Postal Code">
                     <FlowPane.margin>
                        <Insets right="25.0" top="10.0" />
                     </FlowPane.margin>
                  </TextField>
                  <ComboBox fx:id="comboCountry" prefHeight="25.0" prefWidth="170.0" promptText="Country">
                     <FlowPane.margin>
                        <Insets top="10.0" />
                     </FlowPane.margin>
                  </ComboBox>
               </children>
            </FlowPane>
            <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2" />
            <FlowPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Button fx:id="btnSave" mnemonicParsing="false" onAction="#btnSave_onAction" prefHeight="25.0" prefWidth="52.0" text="Save">
                     <FlowPane.margin>
                        <Insets left="150.0" />
                     </FlowPane.margin>
                  </Button>
                  <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#btnCancel_onAction" text="Cancel">
                     <FlowPane.margin>
                        <Insets left="20.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
            </FlowPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
